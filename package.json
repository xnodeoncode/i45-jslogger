{
  "name": "i45-jslogger",
  "version": "1.3.0",
  "description": "A simple JavaScript logger for capturing events in the console and local storage.",
  "main": "./dist/logger.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "checkin": "git add . && git commit -m 'See CHANGES.md for a list of changes.'",
    "build": "cp -r src/logger.js dist/ && cp -r src/iLogger.js dist/ && cp -r src/iLoggerValidator.js dist/",
    "dev": "vite",
    "vite-build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "files": [
